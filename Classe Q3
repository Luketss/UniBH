package classexobjetoex1;


public class Aluno {
    public String nome;
    public String sexo;
    public int ra;
    public String endereco;
    public String email;
    public int idade;
    
    public Aluno (String nomeParametro, String sexoParametro, int raParametro, String enderecoParametro, String emailsParametro, int idadeParametro){
        this.nome = nomeParametro;
        this.sexo = sexoParametro;
        this.ra = raParametro;
        this.endereco = enderecoParametro;
        this.idade = idadeParametro;
    }
    
}


package classexobjetoex1;


public class ClasseXobjetoEx1 {

    
    public static void main(String[] args) {
        
        Aluno A1 = new Aluno("Lucas", "M", 11916079, "Rua Po√ßos de Caldas", "lucas@gmail.com", 21);
